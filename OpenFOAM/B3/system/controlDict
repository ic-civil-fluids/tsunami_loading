FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}

startFrom       startTime;

startTime       131;

stopAt          endTime;

endTime         139;

deltaT          0.001;

writeControl    adjustableRunTime;

writeInterval   0.01;

purgeWrite      701;

writeFormat     ascii;

writePrecision  8;

writeCompression off;

timeFormat      general;

timePrecision   8;

runTimeModifiable no;

adjustTimeStep  yes;

maxCo           0.20;

maxAlphaCo      0.10;

maxDeltaT       0.01;

